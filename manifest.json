{
    "name": "Prestashop",
    "id": "prestashop",
    "packaging_format": 1,
    "description": {
        "en": "Create a E-commerce Website",
        "fr": "Créer un site ecommerce"
    },
    "version": "1.7.8.6~ynh1",
    "url": "https://www.prestashop.com/",
    "upstream": {
        "license": "OSL-3.0",
        "website": "https://prestashop.com",
        "demo": "https://demo.prestashop.com/#/en/front",
        "userdoc": "https://doc.prestashop.com/display/PS17/Guide+de+l'utilisateur",
        "code": "https://github.com/PrestaShop/PrestaShop"
    },
    "license": "OSL-3.0",
    "maintainer": {
        "name": "frju365",
        "email": "abld@abld.info"
    },
    "previous_maintainers": [
      {
        "name": "cyp",
        "email": "cyp@rouquin.me",
        "url": ""
      }
    ],
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php8.0-fpm",
        "mysql"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/prestashop",
                "default": "/prestashop"
            },
            {
                "name": "admin",
                "type": "user"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            },
            {
                "name": "language",
                "type": "select",
                "choices": ["English", "Français"],
                "default": "Français"
            },
            {
                "name": "shopname",
                "type": "string"
            },
            {
                "name": "admin",
                "type": "user"
            },
            {
                "name": "password",
                "type": "password"
            }
        ]
    }
}
